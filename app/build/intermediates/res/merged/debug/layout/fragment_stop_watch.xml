<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.neonto.exportedByUser_s2461812465168012.MyApp.StopWatchFragment" >
        <com.neonto.exportedByUser_s2461812465168012.MyApp.StopWatchBackgroundShapeView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mBackgroundShape"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="0.0mm"
        />
    
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal"
            android:textSize="54.8dp"
            android:textStyle="bold"
            android:id="@+id/mText"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="87.29px"
        />
    
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="left|center_vertical"
            android:textSize="0.0dp"
            android:src="@drawable/button_icon729062"
            android:scaleType="fitCenter"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:id="@+id/mSaveButton"
            android:layout_marginLeft="633.74px"
            android:layout_marginTop="302.84px"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/primary"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/stop_watch_reset_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mResetButton"
            android:layout_marginLeft="118.48px"
            android:layout_marginTop="389.06px"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/primary"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/stop_watch_start_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mStartButton"
            android:layout_marginLeft="389.0px"
            android:layout_marginTop="389.06px"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/primary"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/stop_watch_lap_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mLapButton"
            android:layout_marginLeft="118.48px"
            android:layout_marginTop="389.06px"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/primary"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/stop_watch_stop_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mStopButton"
            android:layout_marginLeft="389.0px"
            android:layout_marginTop="389.06px"
        />
    
        <ListView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:listSelector="@android:color/transparent"
            android:divider="@null"
            android:dividerHeight="0dp"
            android:id="@+id/mList"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="518.39px"
        />
    
</RelativeLayout>
