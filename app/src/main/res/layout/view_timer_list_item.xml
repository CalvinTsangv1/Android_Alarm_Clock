<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="109.0dp"
    android:layout_height="158.5dp"
    android:background="@android:color/transparent"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="com.neonto.exportedByUser_s2461812465168012.MyApp.TimerListItemView" >
        <com.neonto.exportedByUser_s2461812465168012.MyApp.TimerListItemRectangleView
            android:layout_width="match_parent"
            android:layout_height="212.52px"
            android:id="@+id/mRectangle"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="0.0mm"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/timer_list_item_reset_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mResetButton"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="12.35mm"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/timer_list_item_start_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mStartButton"
            android:layout_marginLeft="104.0px"
            android:layout_marginTop="12.35mm"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/timer_list_item_stop_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mStopButton"
            android:layout_marginLeft="104.0px"
            android:layout_marginTop="12.35mm"
        />
    
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal|center_vertical"
            android:textSize="15.6dp"
            android:background="@drawable/timer_list_item_add_minute_button"
            android:paddingLeft="4.3px"
            android:paddingRight="4.3px"
            android:paddingTop="-6.8dp"
            android:id="@+id/mAddMinuteButton"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="12.35mm"
        />
    
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/secondary_text"
            android:gravity="left"
            android:textSize="15.6dp"
            android:id="@+id/mNoText"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="0.0mm"
        />
    
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/base_text"
            android:gravity="center_horizontal"
            android:textSize="48.9dp"
            android:textStyle="bold"
            android:id="@+id/mTimerText"
            android:layout_marginLeft="0.0mm"
            android:layout_marginTop="1.76mm"
        />
    
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/base_text"
            android:gravity="left|center_vertical"
            android:textSize="0.0dp"
            android:src="@drawable/button_icon156974"
            android:scaleType="fitCenter"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="0.0px"
            android:paddingRight="0.0px"
            android:id="@+id/mDeleteButton"
            android:layout_marginLeft="127.49px"
            android:layout_marginTop="3.53mm"
        />
    
</RelativeLayout>
